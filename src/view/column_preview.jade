h4 Column Preview
div(class="table-responsive")
    table(id="sheet-preview-" + index, class="table table-bordered table-hover table-striped")
        thead
            tr
                th="Header"
                    span(class="badge pull-right")="row " + 1
                th="First"
                    span(class="badge pull-right")="row " + sheet.header_info.first_position
                th="Middle"
                    span(class="badge pull-right")="row " + sheet.header_info.middle_position
                th="Last"
                    span(class="badge pull-right")="row " + sheet.header_info.last_position
        tbody
            - var preview_char_limit = 25
            each row, index in sheet.column_preview
                - var key = Object.keys(row)[0]
                - var unique_values = row[key][5]
                tr
                    td=key
                    td(title=row[key][0], data-toggle="tooltip")
                        if row[key][0].length > preview_char_limit
                            =row[key][0].toString().substr(0, preview_char_limit) + '...'
                        else
                            =row[key][0]
                    td(title=row[key][1], data-toggle="tooltip")
                        if row[key][1].length > preview_char_limit
                            =row[key][1].toString().substr(0, preview_char_limit) + '...'
                        else
                            =row[key][1]
                    td(title=row[key][2], data-toggle="tooltip")
                        if row[key][2].length > preview_char_limit
                            =row[key][2].toString().substr(0, preview_char_limit) + '...'
                        else
                            =row[key][2]